{
	"name": "flow_agency_file_to_synapse",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_file_agency",
						"type": "DatasetReference"
					},
					"name": "fileagency"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_target_agency",
						"type": "DatasetReference"
					},
					"name": "targetagency"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> fileagency",
				"fileagency sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> targetagency"
			]
		}
	}
}